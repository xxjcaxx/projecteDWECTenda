import { getDatos , postComentario} from "../supaservice";
export { getComentaris, callCrearComentario };

async function getComentaris(idProducte) {
  const response = await getDatos(
    "comentaris",
    false,
    `idProducte=eq.${idProducte}`
  );
  return response;
}

async function callCrearComentario(form) {
  
    postComentario("comentaris")

}